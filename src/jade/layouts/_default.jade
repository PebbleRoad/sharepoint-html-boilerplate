doctype
html.no-js(lang='en')
	head
		meta(charset='utf-8')
		meta(http-equiv='X-UA-Compatible', content='IE=edge')
		title SharePoint HTML Boilerplate
		meta(name='description', content='')
		meta(name='viewport', content='width=device-width, initial-scale=1')
		// Place favicon.ico and apple-touch-icon(s) in the root directory 	
		// build:css styles/main.css 
		link(rel='stylesheet', href='styles/main.css')
		// endbuild 
		| 
		<!--[if lte IE 8]>
		link(rel='stylesheet', href='styles/ie.css')
		| 
		<![endif]-->
		// build:js scripts/vendor/modernizr.js 
		script(src='bower_components/modernizr/modernizr.js')
		// endbuild 
	body

		// Suite Bar
		.sp-suite-bar
			a.suite-bar-branding(href="#")

			nav.nav-suite-bar
				a(href="#") Newsfeed
				a(href="#") OneDrive
				a(href="#") Sites
				
				// Button
				.ui-dropdown
					a(href="#")
						| Admin
						em.fa.fa-caret-down.fa-last

					.dropdown.arrow-right
						.dropdown-inner
							a(href="#") Link 1
							a(href="#") Link 2
							.seperator
							a(href="#") Link 3							
							
				// / Button

		// / Suite Bar

		// Global Nav
		.sp-global-nav
			a(href="#") Share
			a(href="#") Follow
			a(href="#") Edit
			a(href="#") Full screen
		// / Global Nav

		// Header
		header(role="banner")
			.container
				a.logo(href="index.html") Logo

				// Nav secondary
				nav.nav-secondary
					a(href="index.html") Home
					a(href="collection.html") Collection
					a(href="#") Staff directory
					a(href="#") Calendar
					a(href="#") Sites
					a(href="#") Happenings
					a(href="#") Newsfeed
				// / Nav secondary

		// / Header


		// Primary Navigation
		nav.nav-primary
			.container
				-for(var i = 0; i < 5; i++){
					a(href="#") Link #{i+1}
				-}
		// / Primary Navigation

		// Content
		.main-content(role="main")
			.container

				block content

		// / Content

		script(src='//ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js')
		script.
			window.jQuery || document.write('<script src="bower_components/jquery/dist/jquery.min.js"><\\/script>')
		// build:js scripts/ui.js 
		script(src='scripts/ui.js ')
		// endbuild 
